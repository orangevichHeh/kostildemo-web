{
    "name": "bubuni/kostildemo",
    "description": "Kostilnie demki dlya rabotyag",
    "type": "project",
    "license": "GPL-3.0-only",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4 || ~8.0.0 || ~8.1.0 || ~8.2.0",
        "iignatov/lightopenid": "^1.3",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-json": "*",
        "kruzya/steam-id-converter": "^1.0",
        "pimple/pimple": "^3.0"
    },
    "suggest": {
        "ext-zip": "For compressing demo records in ZIP"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/iignatov/LightOpenID"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src\\App"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.7.7",
        "symfony/var-dumper": "^5.4.48",
        "nikic/php-parser": "^4.14"
    }
}
